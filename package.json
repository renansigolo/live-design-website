{
  "name": "live-design",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm -rf package-lock.json node_modules .next",
    "dev": "next dev",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "next lint --fix && stylelint '**/*.scss' --fix",
    "lint": "next lint && stylelint '**/*.scss'",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "start": "next start",
    "svgo": "npx svgo --multipass -f public/images -r"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@materializecss/materialize": "1.1.0-alpha",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sharp": "0.30.1"
  },
  "devDependencies": {
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.3.0",
    "next-sitemap": "2.4.3",
    "postcss": "8.4.6",
    "postcss-scss": "4.0.3",
    "prettier": "2.5.1",
    "sass": "1.49.8",
    "stylelint": "14.5.1",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard-scss": "3.0.0"
  },
  "engines": {
    "node": "^14.15.0 || >=16.13.0",
    "npm": ">=6.14.8",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 11"
  ]
}
