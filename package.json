  "scripts": {
    "postversion": "npm run deploy",
    "clean": "rm -rf .firebase package-lock.json node_modules dist",
    "clean:install": "npm run clean && npm i",
    "build": "NODE_ENV=development gulp",
    "build:prod": "NODE_ENV=production gulp",
    "start": "NODE_ENV=development gulp watch",
    "server": "firebase serve",
    "deploy": "npm run build:prod && firebase deploy",
    "prettier": "prettier --config ./.prettierrc.json --write '**/*.{html,scss,js,json,yml,md}'"
  },
  "dependencies": {
    "firebase": "^7.19.1",
    "materialize-css": "~1.0.0",
    "smooth-scroll": "^16.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "browser-sync": "^2.26.12",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
