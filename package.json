{
  "name": "live-design",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm -rf package-lock.json node_modules .next",
    "dev": "next dev",
    "fix": "next lint --fix && stylelint '**/*.scss' --fix",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint": "next lint && stylelint '**/*.scss'",
    "postversion": "git push && git push --tags",
    "postbuild": "next-sitemap",
    "start": "next start"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@materializecss/materialize": "^1.1.0-alpha",
